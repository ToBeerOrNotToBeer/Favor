@model Favor.Data.User

<html lang="en">
<head>
</head>
<body class="profile-page">
    <br />
    <br />
    <br />

    <div class="wrapper">

        <div class="container header header-filter" style="background-image: url(../../Content/img/coolShit.png);
            margin: -600px -500px -50px -5px; ">
        </div>
        <div class="row">
            <div class="col-md-12 main main-raised"
                 style="margin: -50px 10px 10px 10px">

                <div class="profile" style="margin: -40px 10px 10px 10px">
                    <div class="name text-center">
                        <button class="btn btn-white text-center">
                            <h1 class="title text-center">@Model.FullName</h1>
                        </button><br />


                        <div class="input text-center">

                            <div class="footer text-center">
                                <form action="#">
                                    <input type="submit" value="Send Message" class="btn btn-primary btn-round" data-toggle="tooltip" data-placement="top" title="Send Message to this user">
                                </form>
                            </div>
                            <hr />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div></div>

</body>
